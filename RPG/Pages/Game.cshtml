@page
@model RPG.Pages.GameModel
@{
    ViewData["Title"] = "Game";
}

    <div class="text-center">
        <p>@Model.game.player.currentPlace.Description</p>
        @foreach (RPG.Models.Place place in Model.game.player.currentPlace.TravelPsb)
        {
            <div><a asp-page-handler="Travel" asp-route-placeName="@place.Name" class="btn btn-primary">@place.Name</a></div>
        }
    </div>
